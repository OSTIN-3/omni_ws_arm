<?xml version="1.0" ?>
<robot name="jdamr200">
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    <material name="yellow">
        <color rgba="1 1 0 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>

    <link name="base_footprint"/>

    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="3.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.010" ixy="0.0" ixz="0.0" 
                      iyy="0.010" iyz="0.0" 
                      izz="0.010"/>
        </inertial>
    </link>

    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.075" rpy="0 0 0"/>
    </joint>
    
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel_link" />
        <origin xyz="0 0.175 -0.008" rpy="0 0 0"/>
        <axis xyz="0 1 0" />
    </joint>
    <link name="left_wheel_link">
        <visual>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.0625"/>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <inertia ixx="0.00009" ixy="0.0" ixz="0.0" iyy="0.00009" iyz="0.0" izz="0.00009"/>
        </inertial>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel_link" />
        <origin xyz="0 -0.175 -0.008" rpy="0 0 0"/>
        <axis xyz="0 1 0" />
    </joint>
    <link name="right_wheel_link">
        <visual>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.075"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.0625"/>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <inertia ixx="0.00009" ixy="0.0" ixz="0.0" iyy="0.00009" iyz="0.0" izz="0.00009"/>
        </inertial>
    </link>

    <joint name="caster_front_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_link_front"/>
        <origin xyz="0.2 -0.0 -0.005" rpy="0 0 0"/>
    </joint>
    <link name="caster_link_front">
        <visual>
            <geometry><sphere radius="0.03"/></geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry><sphere radius="0.03"/></geometry>
        </collision>
        <inertial>
            <mass value="0.03"/>
            <inertia ixx="0.00002" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.0" izz="0.00002"/>
        </inertial>
    </link>
    
    <joint name="caster_rear_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_link_rear"/>
        <origin xyz="-0.2 -0.0 -0.005" rpy="0 0 0"/>
    </joint>
    <link name="caster_link_rear">
        <visual>
            <geometry><sphere radius="0.03"/></geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry><sphere radius="0.03"/></geometry>
        </collision>
        <inertial>
            <mass value="0.03"/>
            <inertia ixx="0.00002" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.0" izz="0.00002"/>
        </inertial>
    </link>

    <joint name="laser_joint" type="fixed">
        <origin xyz="0.3 0 0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="laser_frame"/> 
    </joint>
    <link name="laser_frame">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.025"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.025"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.05"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.000006" ixy="0.0" ixz="0.0" iyy="0.000006" iyz="0.0" izz="0.000003"/>
        </inertial>
    </link>

    <joint name="imu_joint" type="fixed">
        <origin xyz="0.0 0.0 0.075" rpy="0 0 0"/> 
        <parent link="base_link"/>
        <child link="imu_link"/>
    </joint>
    <link name="imu_link">
        <inertial>
            <mass value="0.01"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.000001" ixy="0.0" ixz="0.0" iyy="0.000001" iyz="0.0" izz="0.000001"/>
        </inertial>
        <visual>
             <geometry>
                 <box size="0.01 0.01 0.01"/>
             </geometry>
             <material name="blue"/>
        </visual>
    </link>

</robot>
